import java.util.*;
class Sample1
{
int a=45;
static int b=87;
void show()
{
System.out.println("Instance:"+a+" "+b);
}
Sample1()
{
System.out.println("Constructor:"+a+" "+b);
}
static void display()
{
Sample1 t=new Sample1();
System.out.println("Static:"+t.a+" "+b);
}
}
class Sample
{
public static void main(String args[])
{
Sample1.display();
Sample1 s1=new Sample1();
s1.show();
s1.display();
}
}
